{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.ts",
      "use": "@nestjs/cli"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "src/routes/api/graphql",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
